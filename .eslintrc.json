{
  "root": true,
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "shared-node-browser": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2022
  },
  "globals": {
    "global": "readonly",
    "globalThis": "readonly",
    "process": true,
    "prompt": true,
    "setImmediate": true,
    "clearImmediate": true
  },
  "rules": {}
}